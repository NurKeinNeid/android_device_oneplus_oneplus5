genfscon proc  /touchpanel u:object_r:proc_touchpanel:s0

genfscon proc  /flicker_free u:object_r:proc_flicker_free:s0

# Display Mode
genfscon sysfs /devices/virtual/graphics/fb0/srgb                u:object_r:sysfs_dm:s0
genfscon sysfs /devices/virtual/graphics/fb0/hbm                 u:object_r:sysfs_dm:s0
genfscon sysfs /devices/virtual/graphics/fb0/dci_p3              u:object_r:sysfs_dm:s0
genfscon sysfs /devices/virtual/graphics/fb0/panel_serial_number u:object_r:sysfs_dm:s0

